{
  "connector": "local",
  "extends": ["accessibility"],
  "formatters": ["html", "summary"],
  "hints": [
    "apple-touch-icons",
    "button-type",
    "compat-api/css",
    "compat-api/html",
    "create-element-svg",
    "css-prefix-order",
    "disown-opener",
    "highest-available-document-mode",
    "leading-dot-classlist",
    "manifest-exists",
    "meta-charset-utf-8",
    "meta-viewport",
    "no-bom",
    "no-inline-styles",
    "no-protocol-relative-urls",
    "scoped-svg-styles",
    "typescript-config/consistent-casing",
    "typescript-config/is-valid",
    "typescript-config/no-comments",
    "typescript-config/target"
  ],
  "hintsTimeout": 10000,
  "parsers": [
    "babel-config",
    "css",
    "html",
    "javascript",
    "jsx",
    "sass",
    "typescript",
    "typescript-config",
    "webpack-config"
  ]
}
